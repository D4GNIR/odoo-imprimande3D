# Fichier d'exemple pour les variables d'environnement
# Copiez ce fichier vers .env et modifiez les valeurs

# Base de données PostgreSQL
POSTGRES_DB=jeje-odoo
POSTGRES_USER=jeje-odoo
POSTGRES_PASSWORD=jeje-odoo

# Odoo
ODOO_ADMIN_PASSWORD=jeje-odoo

# Ports (modifiez si ces ports sont déjà utilisés)
ODOO_PORT=8069
POSTGRES_PORT=5432

# Configuration avancée
ODOO_WORKERS=0
ODOO_LOG_LEVEL=info
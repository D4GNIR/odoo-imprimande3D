# Fichier d'exemple pour les variables d'environnement
# Copiez ce fichier vers .env et modifiez les valeurs

# Base de données PostgreSQL
POSTGRES_DB=odoo_imprimante3d
POSTGRES_USER=odoo
POSTGRES_PASSWORD=changez_ce_mot_de_passe_db

# Odoo
ODOO_ADMIN_PASSWORD=changez_ce_master_password

# Ports (modifiez si ces ports sont déjà utilisés)
ODOO_PORT=8069
POSTGRES_PORT=5432

# Configuration avancée
ODOO_WORKERS=0
ODOO_LOG_LEVEL=info